{
  "name": "nodepop",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "node ./bin/www",
    "dev": "cross-env DEBUG=nodepop:* ./node_modules/.bin/nodemon ./bin/www",
    "eslint": "./node_modules/.bin/eslint .",
    "generate-certs": "mkcert localhost 127.0.0.1 ::1 && mv ./*.pem ./certificates",
    "install-db": "node ./scripts/install_db",
    "pm2": "./node_modules/.bin/pm2",
    "pm2-start": "./node_modules/.bin/pm2 start ecosystem.config.js",
    "start": "cross-env DEBUG=nodepop:* ./node_modules/.bin/nodemon ./bin/www",
    "test": "./node_modules/.bin/mocha **/*.e2e.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.5",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.3",
    "cote": "^0.20.0",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "fs-extra": "^7.0.1",
    "http-errors": "~1.6.2",
    "i18n": "^0.8.3",
    "jimp": "^0.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^6.1.2",
    "nodemon": "^1.18.9",
    "pm2": "^3.5.0",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2"
  }
}
